<% if(success.length > 0){%> <% console.log(success[0])%>
<div class="container p-4">
	<div class="row">
		<div class="col-md-4 mx-auto">
			<div class="alert alert-success alert-dismissible fade show" role="alert">
				<%= success[0] %>
				<button
					type="button"
					class="btn-close"
					data-bs-dismiss="alert"
					aria-label="Close"
				></button>
			</div>
		</div>
	</div>
</div>
<% } %> <% if(message.length > 0){ %>
<div class="container p-4">
	<div class="row">
		<div class="col-md-4 mx-auto">
			<div class="alert alert-danger alert-dismissible fade show" role="alert">
				<%= message[0] %>
				<button
					type="button"
					class="btn-close"
					data-bs-dismiss="alert"
					aria-label="Close"
				></button>
			</div>
		</div>
	</div>
</div>
<% } %>
